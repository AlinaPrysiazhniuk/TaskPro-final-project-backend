  "components": {
    "schemas": {
      "RegistrationRequest": {
        "type": "object",
        "required": ["name", "email", "password"],
        "properties": {
          "name": {
            "type": "string",
            "description": "User's name",
            "format": "name"
          },
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "example": "qwerty123"
          }
        }
      },
      "RegistrationResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "description": "JWT token"
          },
          "email": {
            "type": "string",
            "description": "User's email"
          },
          "name": {
            "type": "string",
            "description": "User's name"
          },
          "avatarURL": {
            "type": "string",
            "description": "User's avatar"
          },
          "userId": {
            "type": "string",
            "description": "User's id"
          },
          "theme": {
            "type": "string",
            "description": "User's theme"
          }
        }
      },
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "example": "qwerty123"
          }
        }
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "description": "JWT token"
          },
          "email": {
            "type": "string",
            "description": "User's email"
          },
          "name": {
            "type": "string",
            "description": "User's name"
          },
          "avatarURL": {
            "type": "string",
            "description": "User's avatar"
          },
          "userId": {
            "type": "string",
            "description": "User's id"
          },
          "theme": {
            "type": "string",
            "description": "User's theme"
          }
        }
      },
      "LogoutRequest": {
        "type": "object",
        "properties": {}
      },
      "LogoutResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Logout success message",
            "example": "You have successfully logged out"
          }
        }
      },
      "ChangeUserThemeRequest": {
        "type": "object",
        "required": ["theme"],
        "properties": {
          "theme": {
            "type": "string",
            "description": "Theme's title",
            "format": "light"
          }
        }
      },
      "ChangeAvatarRequest": {
        "type": "object",
        "required": ["theme"],
        "properties": {
          "theme": {
            "type": "string",
            "format": "binary",
            "description": "The avatar file to upload"
          }
        }
      },
      "ChangeAvatarResponse": {
        "type": "object",
        "properties": {
          "avatarURL": {
            "type": "string",
            "description": "URL of the updated avatar"
          }
        }
      },
      "ChangeUserResponse": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "User's id"
          },
          "name": {
            "type": "string",
            "description": "User's name"
          },
          "password": {
            "type": "string",
            "description": "User's password"
          },
          "email": {
            "type": "string",
            "description": "User's email"
          },
